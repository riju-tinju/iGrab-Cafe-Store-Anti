  <main class="main-container mt-20 lg:mt-24 pb-20 lg:pb-10 px-4 lg:px-8">
    
    <!-- Hero Section -->
    <section class="hero-section mb-8">
      <h1 class="text-4xl lg:text-5xl font-bold mb-4">Frequently Asked Questions</h1>
      <p class="text-lg max-w-xl mx-auto">Find answers to the most common questions about our products, services, and more.</p>
    </section>

    <!-- FAQ Search Section -->
    <section class="mb-8 lg:mb-10">
      <div class="search-container max-w-lg mx-auto">
