<%- include('../../partials/Header') %>
  <!-- Main Content -->
   <head>
    <style>
         /* FAQ Accordion Styles */
    .faq-item {
      border-radius: 12px;
      overflow: hidden;
      margin-bottom: 1rem;
      box-shadow: 0 2px 8px rgba(0, 0, 0, 0.05);
      background-color: white;
      transition: all 0.3s ease;
    }

    .faq-question {
      padding: 1.25rem;
      cursor: pointer;
      display: flex;
      justify-content: space-between;
      align-items: center;
      font-weight: 600;
      font-family: 'DM Sans', sans-serif;
    }

    .faq-answer {
      padding: 0;
      max-height: 0;
      overflow: hidden;
      transition: all 0.3s ease;
      border-top: 0px solid #f0f0f0;
    }

    .faq-answer.active {
      padding: 0 1.25rem 1.25rem 1.25rem;
      max-height: 500px;
      border-top: 1px solid #f0f0f0;
    }

    .faq-category {
      padding: 1rem 1.5rem;
      background-color: var(--accent-beige);
      border-radius: 12px;
      margin-bottom: 1.5rem;
      font-family: 'Chillax', sans-serif;
      font-weight: 700;
      color: var(--primary-green);
    }

    /* Category Pills */
    .category-pill {
      background-color: white;
      border-radius: 30px;
      padding: 0.5rem 1.25rem;
      box-shadow: 0 2px 8px rgba(0, 0, 0, 0.05);
      transition: all 0.3s ease;
      cursor: pointer;
      white-space: nowrap;
    }

    .category-pill:hover,
    .category-pill.active {
      background-color: var(--primary-green);
      color: white;
    }

    /* Toast Notification */
    .toast {
      position: fixed;
      top: 20px;
      left: 50%;
      transform: translateX(-50%);
      background-color: rgba(0, 0, 0, 0.8);
      color: white;
      padding: 1rem 1.5rem;
      border-radius: 30px;
      z-index: 9999;
      opacity: 0;
      transition: opacity 0.3s ease;
    }

    .toast.show {
      opacity: 1;
    }

    /* No Results Message */
    .no-results {
      display: none;
      text-align: center;
      padding: 3rem 0;
      font-family: 'DM Sans', sans-serif;
    }

    /* Hero Section */
    .hero-section {
      background: linear-gradient(rgba(0, 63, 46, 0.85), rgba(0, 63, 46, 0.85)), url("https://images.unsplash.com/photo-1495474472287-4d71bcdd2085?ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D&auto=format&fit=crop&w=1170&q=80");
      background-size: cover;
      background-position: center;
      padding: 4rem 1rem;
      color: white;
      text-align: center;
      border-radius: 0 0 20px 20px;
    }

    /* Desktop Optimizations */
    @media (min-width: 1024px) {

        .main-container {
        margin-top: 80px;
        padding: 0 2rem;
      }

      .faq-container {
        max-width: 900px;
        margin: 0 auto;
      }
    }

    /* Hide questions when filtered out */
    .faq-item.hidden {
      display: none;
    }

    /* Category filtering */
    .category-content {
      display: none;
    }

    .category-content.active {
      display: block;
    }
    </style>
   </head>
